{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.next": true,
    "**/.agents": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true,
    "**/.cache": true,
    "**/test-results": true,
    "**/tmp": true,
    "**/*.log": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.next": true,
    "**/.agents": true,
    "**/dist": true,
    "**/build": true,
    "**/out": true,
    "**/.cache": true,
    "**/test-results": true,
    "**/tmp": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.next/*/**": true,
    "**/.agents/*/**": true,
    "**/dist/*/**": true,
    "**/build/*/**": true,
    "**/out/*/**": true,
    "**/.cache/*/**": true,
    "**/test-results/*/**": true,
    "**/tmp/*/**": true,
    "**/*.log": true
  },
  "typescript.tsserver.maxTsServerMemory": 8192,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsserver.useSeparateSyntaxServer": "never"
}
